package queue;



public class Operasi {
	
	private Node FRONT;
	private Node REAR;
	
	public void enqueue(int data) {
		Node newNode = new Node(data);
		if(isEmpty()) {
			FRONT = newNode;
			REAR = newNode;
			
		} else {
			REAR.setNext(newNode);
			REAR = newNode;
		}
	}
	
	public int dequeue() {
		
		return 0;
	}
	
	public boolean isEmpty() {
		if(FRONT == null) {
			return true;
		}
		return false;
	}
}
