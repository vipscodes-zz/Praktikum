package permasalahan2test;

public class Operasi {
	private Stack stack;
	private String input;
	
	public Operasi(String input) {
		this.input = input;
	}
	public String toPostfix() {
		stack = new Stack();
		char ch;
		
		int bil1, bil2, hasilOps;
		
			for (int i = 0; i < input.length(); i++) {
				ch = input.charAt(i);
				stack.tampilData();
				if(ch >= '0' && ch <='9') {
					stack.push((int)(ch-'0'));
				}
			}
		
		
		return "";
	}
	
}
